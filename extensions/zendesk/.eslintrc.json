{
  "root": true,
  "extends": [],
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-redeclare": "off",
    "no-unexpected-multiline": "off",
    "no-unreachable": "off",
    "no-constant-condition": "off",
    "no-empty": "off"
  }
}
